{
    "configurations": [
        {
            "name": "CrossARM",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "__ARMCC_VERSION",
                "CORE_M0PLUS"
            ],
            //"forcedInclude": ["${workspaceFolder}/src/someFile.h"],
